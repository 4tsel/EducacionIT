<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cross Browser Testing">
	<test name="Cross Browser Test - CHROME">
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="tests.CrossBrowserTest">
				<methods>
					<include name="testValidLogin" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Cross Browser Test - FIREFOX">
		<parameter name="browser" value="Firefox" />
		<classes>
			<class name="tests.CrossBrowserTest">
				<methods>
					<include name="testInvalidLogin" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Cross Browser Test - EDGE">
		<parameter name="browser" value="Edge" />
		<classes>
			<class name="tests.Laboratorio5_E3">
				<methods>
					<include name="testAlertManipulation" />
				</methods>
			</class>
		</classes>
	</test>
</suite>
